    "node": "16"
  },
  "scripts": {
    "start": "nodemon -r esm server/index.js",
    "build": "cd client && npm install && npm run build",
    "heroku-postbuild": "PGSSLMODE=no-verify NODE_ENV=production npm ci && knex migrate:latest && cd client && npm ci && npm run build",
